[
  {
    "environment": [{
      "name": "hostname",
      "value": "content-service"
    },
    {
      "name": "name",
      "value": "content-service"
    }],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/fargate/service/content-service/content-service-prod",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [{
      "containerPort": 8000,
      "hostPort": 8000
    }],
    "essential": true,
    "image": "245402993223.dkr.ecr.us-east-1.amazonaws.com/content-service:latest",
    "memoryReservation": 256,
    "name": "content-service"
  }
]
